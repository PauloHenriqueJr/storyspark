{
  "description": "Create these credentials in n8n with matching names. Replace placeholders with your secrets.",
  "credentials": [
    {
      "type": "httpHeaderAuth",
      "name": "Supabase Service Header",
      "fields": {
        "headers": {
          "apikey": "{{ SUPABASE_SERVICE_ROLE_KEY }}",
          "Authorization": "Bearer {{ SUPABASE_SERVICE_ROLE_KEY }}",
          "Content-Profile": "public",
          "Accept-Profile": "public"
        },
        "baseUrl": "{{ SUPABASE_URL }}"
      }
    },
    {
      "type": "httpHeaderAuth",
      "name": "Resend Edge Function",
      "fields": {
        "headers": {
          "Content-Type": "application/json"
        },
        "baseUrl": "{{ SUPABASE_URL }}/functions/v1"
      }
    },
    {
      "type": "httpHeaderAuth",
      "name": "Stripe Secret",
      "fields": {
        "headers": {
          "Authorization": "Bearer {{ STRIPE_API_KEY }}",
          "Content-Type": "application/json"
        },
        "baseUrl": "https://api.stripe.com"
      }
    }
  ]
}

